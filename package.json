{
   "type": "module",
   "name": "sohl",
   "transform": {},
   "scripts": {
      "build": "npm run build:code && npm run build:css && npm run build:db",
      "build:clean": "node ./utils/packs.mjs package clean",
      "build:code": "rollup sohl.mjs -f es -o sohl-compiled.mjs -p node-resolve -m --sourcemapFile sohl.mjs",
      "build:css": "sass scss/sohl.scss css/sohl.css",
      "build:db": "npm run build:compiledb && npm run build:packdb",
      "build:compiledb": "node ./utils/packs.mjs package compile",
      "build:packdb": "node ./utils/packs.mjs package pack",
      "build:json": "node ./utils/packs.mjs package unpack",
      "lint": "eslint --ext .mjs --ignore-path .gitignore ./",
      "lint:fix": "eslint --ext .mjs --ignore-path .gitignore ./ --fix",
      "postinstall": "npm run build:css && npm run build:db",
      "test:logic": "cross-env NODE_OPTIONS='$NODE_OPTIONS --experimental-vm-modules' jest tests/puppeteer",
      "test:ui": "cypress open"
   },
   "devDependencies": {
      "@biomejs/biome": "^1.9.4",
      "@eslint/js": "^9.4.0",
      "@foundryvtt/foundryvtt-cli": "^1.0.0-rc.4",
      "@rollup/plugin-commonjs": "^28.0.3",
      "@rollup/plugin-node-resolve": "^16.0.1",
      "@rollup/plugin-terser": "^0.4.4",
      "cross-env": "^7.0.3",
      "cypress": "^14.2.0",
      "eslint": "^9.4.0",
      "eslint-plugin-jest": "^28.11.0",
      "eslint-plugin-jsdoc": "^50.6.8",
      "fancy-log": "^2.0.0",
      "globals": "^15.3.0",
      "jest": "^29.7.0",
      "loglevel": "^1.9.2",
      "loglevel-plugin-prefix": "^0.8.4",
      "puppeteer": "^24.4.0",
      "rollup": "^2.79.2",
      "unidecode": "^1.1.0",
      "yaml": "^2.7.0",
      "yargs": "^17.7.2"
   }
}

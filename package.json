{
    "type": "module",
    "name": "sohl",
    "transform": {},
    "scripts": {
        "build": "npm run build:clean && npm run build:css && npm run build:db npm run build:code",
        "build:clean": "node ./utils/packs.mjs package clean",
        "build:code": "node ./utils/build.mjs",
        "build:css": "sass scss/sohl.scss css/sohl.css",
        "build:db": "npm run build:compiledb && npm run build:packdb",
        "build:compiledb": "node ./utils/packs.mjs package compile",
        "build:packdb": "node ./utils/packs.mjs package pack",
        "build:json": "node ./utils/packs.mjs package unpack",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint --ext .mjs --ignore-path .gitignore ./",
        "lint:fix": "eslint --ext .mjs --ignore-path .gitignore ./ --fix",
        "postinstall": "npm run build:css && npm run build:db",
        "test:logic": "cross-env NODE_OPTIONS='$NODE_OPTIONS --experimental-vm-modules' jest tests/puppeteer",
        "test:ui": "cypress open",
        "release": "node ./utils/release.mjs",
        "clean:types": "rm -rf build/generated",
        "build:types": "tsc -p tsconfig.json",
        "types": "npm run clean:types && npm run build:types"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@foundryvtt/foundryvtt-cli": "^1.0.0-rc.4",
        "@league-of-foundry-developers/foundry-vtt-types": "^12.331.3-beta",
        "@rollup/plugin-commonjs": "^28.0.3",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@typescript-eslint/parser": "^8.27.0",
        "acorn": "^8.14.1",
        "acorn-walk": "^8.3.4",
        "cross-env": "^7.0.3",
        "cypress": "^14.2.0",
        "eslint": "^9.4.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-jest": "^28.11.0",
        "eslint-plugin-jsdoc": "^50.6.9",
        "fancy-log": "^2.0.0",
        "globals": "^15.3.0",
        "jest": "^29.7.0",
        "loglevel": "^1.9.2",
        "loglevel-plugin-prefix": "^0.8.4",
        "prettier": "^3.5.3",
        "puppeteer": "^24.4.0",
        "rollup": "^2.79.2",
        "unidecode": "^1.1.0",
        "yaml": "^2.7.0",
        "yargs": "^17.7.2"
    },
    "dependencies": {
        "typescript": "^5.8.2"
    }
}

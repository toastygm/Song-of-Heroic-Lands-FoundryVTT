{{#if printable}}
<html>
    <head>
        <link href="fonts/fontawesome/css/all.min.css" rel="stylesheet" type="text/css" media="all">
        <link href="css/style.css" rel="stylesheet" type="text/css" media="all">
        <link href="systems/sohl/css/hmk.css" rel="stylesheet" type="text/css" media="all"><div class="hmk actor sheet flexcol" autocomplete="off">
    </head>
    <body class="sheet" style="overflow: auto; position: relative; background: white">
        {{!-- Sheet Body --}}
        <div class="sheet-body print-window-content">
{{else}}
<form class="{{cssClass}} sheet flexcol" autocomplete="off">
    {{!-- Sheet Header --}}
    {{#if (or isAnimateEntity)}}
        <header class="sheet-header sheet-header-entity">
            <div class="flexrow">
                <img class="profile-img" src="{{actor.img}}" data-edit="img" title="{{actor.name}}" />
                <div class="flexcol header-fields">
                    <div class="flexrow name-attribute">
                        <h1 class="charname"><input name="name" type="text" value="{{actor.name}}" placeholder="Name" data-dtype="String" /></h1>
                        <div class="effect-buttons flexcol" data-tooltip-direction="DOWN">
                            <div class="flexrow">
                                <button class="effect-button {{#if effectStatus.auralshock}}active{{/if}}" name="auralshock" type="button" data-status-id="auralshock" data-tooltip="Aural Shock">AShk</button>
                                <button class="effect-button toggle-status-effect {{#if effectStatus.sleep}}active{{/if}}" name="sleep" type="button" data-status-id="sleep" data-tooltip="Sleep">SLP</button>
                                <button class="effect-button toggle-status-effect {{#if effectStatus.prone}}active{{/if}}" name="prone" type="button" data-status-id="prone" data-tooltip="Prone">PRN</button>
                                <button class="effect-button toggle-status-effect {{#if effectStatus.stun}}active{{/if}}" name="stun" type="button" data-status-id="stun" data-tooltip="Stun">STN</button>
                            </div>
                            <div class="flexrow">
                                <button class="effect-button {{#if effectStatus.fatigue}}active{{/if}}" name="fatigue" type="button" data-status-id="fatigue" data-tooltip="Fatigue">Fatg</button>
                                <button class="effect-button toggle-status-effect {{#if effectStatus.incapacitated}}active{{/if}}" name="incapacitated" type="button" data-status-id="incapacitated" data-tooltip="Incapacitated">INC</button>
                                <button class="effect-button toggle-status-effect {{#if effectStatus.unconscious}}active{{/if}}" name="unconscious" type="button" data-status-id="unconscious" data-tooltip="Unconscious">UNC</button>
                                <button class="effect-button toggle-status-effect {{#if effectStatus.dead}}active{{/if}}" name="dead" type="button" data-status-id="dead" data-tooltip="Dead">KIA</button>
                            </div>
                        </div>
                        <div class="flexcol healtharea">
                            {{#if (eq variant "mistyisle")}}
                            <div class="health flexrow">
                                <span class="label">UP: {{adata.$univPenalty.effective}}</span>
                                <span class="label">PP: {{adata.$physPenalty.effective}}</span>
                            </div>
                            {{/if}}
                            <div class="health flexrow">
                                <span class="label">Health: {{adata.health.pct}}%</span>
                            </div>
                            <div class="healthbar">
                                <span class="bar" style="width: {{adata.health.pct}}%"></span>
                            </div>
                            <div class="empty"></div>
                        </div>
                    </div>
                    <div class="bodypart-statuses grid grid-6col">
                        {{#each itemTypes.bodypart as | item |}}
                            <li class="bodypart-status {{#if system.$isUnusable}}unusable{{else}}{{#if system.$impairment.effective}}impaired{{else}}none{{/if}}{{/if}}">
                                <span>{{item.name}}</span>
                            </li>
                        {{/each}}
                    </div>
                </div>
            </div>
        </header>

        {{!-- Sheet Tab Navigation --}}
        <nav class="sheet-tabs tabs" data-group="primary">
            <a class="item" data-tab="facade">Fa&#231;ade</a>
            {{#unless limited}}
            <a class="item" data-tab="profile">Profile</a>
            <a class="item" data-tab="skills">Skills</a>
            <a class="item" data-tab="combat">Combat</a>
            <a class="item" data-tab="trauma">Trauma</a>
            <a class="item" data-tab="mysteries">Mysteries</a>
            <a class="item" data-tab="inventory">Gear</a>
            <a class="item" data-tab="actions">Actions</a>
            <a class="item" data-tab="effects">Effects</a>
            {{/unless}}
        </nav>
    {{else}}
        <header class="sheet-header sheet-header-object">
            <div class="flexrow">
                <img class="profile-img" src="{{actor.img}}" data-edit="img" title="{{actor.name}}" height="100" width="100" />
                <div class="flexcol header-fields">
                    <div class="flexrow name-attribute">
                        <h1 class="charname"><input name="name" type="text" value="{{actor.name}}" placeholder="Name" /></h1>
                    </div>
                    <div class="header-attributes">
                    </div>
                </div>
            </div>
        </header>

        {{!-- Sheet Tab Navigation --}}
        <nav class="sheet-tabs tabs" data-group="primary">
            <a class="item" data-tab="facade">Fa&#231;ade</a>
            {{#unless limited}}
            <a class="item" data-tab="inventory">Contents</a>
            {{/unless}}
        </nav>
    {{/if}}

    {{!-- Sheet Body --}}
    <section class="sheet-body">
{{/if}}

        {{!-- Facade Tab --}}
        {{#if printable}}
            <div class="facade">
                <h1>Fa√ßade</h1>
                <div class="flexrow">
                    <img class="facade-image" src="{{adata.bioImage}}" data-edit="system.bioImage" title="{{actor.name}}"
                        height="300" width="200" />
                    <div class="facade-description">
                        {{{adata.description}}}
                    </div>
                </div>
            </div>
        {{else}}
            <div class="tab facade" data-group="primary" data-tab="facade">
                <div class="flexrow">
                    <img class="facade-image" src="{{adata.bioImage}}" data-edit="system.bioImage" title="{{actor.name}}"
                        height="300" width="200" />
                    <div class="facade-description">
                        {{{editor adata.description target="system.description" button=true owner=owner editable=editable}}}
                    </div>
                </div>
            </div>
        {{/if}}

        {{#unless limited}}
            {{#if isAnimateEntity}}
                {{!-- Profile Tab --}}
                {{#unless printable}}
                <div class="tab profile" data-group="primary" data-tab="profile">
                    <div class="search-items">
                        <input class="search-criteria" type="search" name="search-traits" value="" aria-label="Search Traits"
                            placeholder="Search Traits" autocomplete="off" />
                        <div class="search-controls">
                            <a class="search-control item-create" data-tooltip="Create Trait"
                                data-type="trait"><i class="fas fa-file-plus"></i></a>
                        </div>
                    </div>
                {{else}}
                <div class="profile" style="break-before: page;">
                    <h1>Profile</h1>
                {{/unless}}
                    <div class="flexcol">
                        {{!-- Attributes --}}
                        <ul class="grid grid-6col attribute-scores item-list" data-category="attribute">
                            {{#each attributes as |attribute| }}
                            <li class="attribute item" data-attribute="{{attribute.name}}" data-item-id="{{attribute.id}}" data-uuid="{{attribute.uuid}}">
                                <div class="flexrow">
                                    <div></div>
                                    <h4 class="name box-title">{{attribute.name}}</h4>
                                    {{#unless printable}}
                                    <div class="item-controls">
                                        <a class="item-control item-contextmenu" title="Menu"><i
                                            class="fas fa-ellipsis-vertical fa-fw"></i></a>
                                    </div>
                                    {{/unless}}
                                </div>
                                <h3 class="value default-action" data-tooltip="{{system.$score.abbrev}}">{{system.$score.effective}}</h3>
                                <h4 class="label">{{system.$score.valueDesc}}</h4>
                                <div class="attribute-modifiers flexrow">
                                    {{#if (eq variant "legendary")}}
                                    <div class="secmod">SM: {{numberFormat system.$masteryLevel.secMod sign=true}}</div>
                                    {{else}}
                                    <div class="secmod"></div>
                                    {{/if}}
                                    <div class="secmod" data-tooltip="{{system.$masteryLevel.abbrev}}">TL: {{system.$masteryLevel.effective}}</div>
                                </div>
                            </li>
                            {{/each}}
                        </ul>

                        {{!-- Traits --}}
                        <div class="flexcol items traits">
                            {{#each itemSubtypes.trait as | itemstlist |}}
                            <div class="items-list traits-list">
                                <header class="items-header trait flexrow">
                                    <h3 class="item-name name">{{itemstlist.label}} Traits</h3>
                                    <div class="detail subtype">Intensity</div>
                                    <div class="detail source">Value</div>
                                    <div class="detail notes">Notes</div>
                                </header>
                                <ol class="item-list">
                                    {{#each itemstlist.items as |item|}}
                                        <li class="item flexrow" data-item-id="{{item.id}}" data-uuid="{{item.uuid}}"
                                            data-item-name="{{item.name}}">
                                            <div class="item-name name default-action flexrow">
                                                <div class="item-image" style="background-image: url('{{item.img}}')"></div>
                                                <h4 data-tooltip="{{item.name}}">{{item.name}}</h4>
                                            </div>
                                            <div class="detail subtype">{{lookup ../config.Item.dataModels.trait.intensities item.system.intensity}}</div>
                                            <div class="detail source">{{item.system.displayVal}}</div>
                                            <div class="detail notes">{{item.system.notes}}</div>
                                            {{#unless printable}}
                                            <div class="item-controls">
                                                <a class="item-control item-contextmenu" title="Menu"><i
                                                    class="fas fa-ellipsis-vertical fa-fw"></i></a>
                                            </div>
                                            {{/unless}}
                                        </li>
                                    {{/each}}
                                </ol>        
                            </div>
                            {{/each}}
                        </div>

                        {{!-- Affiliations --}}
                        <div class="flexcol items affiliations">
                            <ol class="items-list">
                                <li class="items-header flexrow">
                                    <h3 class="item-name name">Affiliations</h3>
                                    <div class="detail ele">Level</div>
                                    <div class="detail source">Society</div>
                                    <div class="detail source">Office</div>
                                    <div class="detail source">Title</div>
                                    <div class="detail notes">Notes</div>
                                    {{#unless printable}}
                                    <div class="item-controls">
                                        <a class="item-control item-create" data-tooltip="Create Affiliation"
                                            data-type="affiliation"><i class="fas fa-file-plus fa-lg"></i></a>
                                    </div>
                                    {{/unless}}
                                </li>
                                <ol class="item-list">
                                    {{#each itemTypes.affiliation as |item|}}
                                        <li class="item flexrow" data-item-id="{{item.id}}" data-uuid="{{item.uuid}}"
                                            data-item-name="{{item.name}}">
                                            <div class="item-name name default-action flexrow">
                                                <div class="item-image" style="background-image: url('{{item.img}}')"></div>
                                                <h4 data-tooltip="{{item.name}}">{{item.name}}</h4>
                                            </div>
                                            <div class="detail ele">{{system.level}}</div>
                                            <div class="detail source">{{system.society}}</div>
                                            <div class="detail source">{{system.office}}</div>
                                            <div class="detail source">{{system.title}}</div>
                                            <div class="detail notes">{{system.notes}}</div>
                                            {{#unless printable}}
                                            <div class="item-controls">
                                                <a class="item-control item-contextmenu" title="Menu"><i
                                                    class="fas fa-ellipsis-vertical fa-fw"></i></a>
                                            </div>
                                            {{/unless}}
                                        </li>
                                    {{/each}}
                                </ol>        
                            </ol>
                        </div>

                        {{!-- Biography --}}
                        <div class="flexcol">
                            <div class="profile-desc flexcol">
                                <h3 class="label">Biography</h3>
                                <div class="description">
                                    {{#unless printable}}
                                    {{editor adata.biography target="system.biography" button=true owner=owner editable=editable}}
                                    {{else}}
                                    {{{adata.biography}}}
                                    {{/unless}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                {{!-- Skills Tab --}}
                {{#unless printable}}
                <div class="tab skills items" data-group="primary" data-tab="skills">

                    <div class="search-items">
                        <input class="search-criteria" type="search" name="search-skills" value="" aria-label="Search Skills"
                            placeholder="Search Skills" autocomplete="off" />
                        <div class="search-controls">
                            <a class="search-control item-create" data-tooltip="Create Skill"
                                data-type="skill"><i class="fas fa-file-plus"></i></a>
                        </div>
                    </div>
                {{else}}
                <div class="skills items flexcol" style="break-before: page;">
                    <h1>Skills</h1>
                {{/unless}}
                    <!-- Skills -->
                    {{#each itemSubtypes.skill as | itemstlist |}}
                    {{#if (and itemstlist.items.length (ne @key "esoteric"))}}
                    <div class="items-list" data-category="{{@key}}">
                        <header class="items-header flexrow">
                            <h3 class="item-name name">{{itemstlist.label}} Skills</h3>
                            <div class="detail ele" data-tooltip="Skill Base">SB</div>
                            <div class="detail ele" data-tooltip="Mastery Level">ML</div>
                            <div class="detail ele" data-tooltip="Index">Index</div>
                            {{#if (eq variant "legendary")}}
                            <div class="detail ele" data-tooltip="Secondary Modifier">SM</div>
                            {{/if}}
                            <div class="detail ele" data-tooltip="Effective Mastery Level">EML</div>
                            <div class="detail ele" data-tooltip="Fate Mastery Level">Fate</div>
                            <div class="detail notes">Notes</div>
                        </header>
                        <ol class="item-list">
                            {{#each itemstlist.items as | item |}}
                                <li class="item flexrow" data-item-id="{{item.id}}" data-uuid="{{item.uuid}}"
                                    data-item-name="{{item.name}}">
                                    <div class="item-name name default-action flexrow">
                                        <div class="item-image" style="background-image: url('{{item.img}}')"></div>
                                        <h4 data-tooltip="{{item.name}}">{{item.name}}</h4>
                                    </div>
                                    <div class="detail ele">{{system.$skillBase.value}}</div>
                                    <div class="detail ele">{{system.$masteryLevel.base}}</div>
                                    <div class="detail ele">
                                        {{#if system.$masteryLevel.disabled}}
                                        <i class="fas fa-xmark fa-lg"></i>
                                        {{else}}
                                        {{system.$masteryLevel.index}}
                                        {{/if}}
                                    </div>
                                    {{#if (eq variant "legendary")}}
                                    <div class="detail ele">
                                        {{#if system.$masteryLevel.disabled}}
                                        <i class="fas fa-xmark fa-lg"></i>
                                        {{else}}
                                        {{numberFormat system.$masteryLevel.secMod sign=true}}
                                        {{/if}}
                                    </div>
                                    {{/if}}
                                    <div class="detail ele" data-tooltip="{{system.$masteryLevel.abbrev}}">
                                        {{#if system.$masteryLevel.disabled}}
                                        <i class="fas fa-xmark fa-lg"></i>
                                        {{else}}
                                        {{system.$masteryLevel.effective}}
                                        {{/if}}
                                    </div>
                                    <div class="detail ele" data-tooltip="{{system.$masteryLevel.fate.abbrev}}">
                                        {{#if system.$masteryLevel.fate.disabled}}
                                        <i class="fas fa-xmark fa-lg"></i>
                                        {{else}}
                                        {{system.$masteryLevel.fate.effective}}
                                        {{/if}}
                                    </div>
                                    <div class="detail notes">{{system.notes}}</div>
                                    {{#unless printable}}
                                    <div class="item-controls">
                                        {{#if system.canImprove}}
                                        <a class="item-control toggle-improve-flag"
                                            title="Improve {{item.label}}">{{#if system.improveFlag}}<i
                                            class="active fas fa-star fa-fw"></i>{{else}}<i
                                            class="far fa-star fa-fw"></i>{{/if}}</a>
                                        {{/if}}
                                        <a class="item-control item-contextmenu" title="Menu"><i
                                            class="fas fa-ellipsis-vertical fa-fw"></i></a>
                                    </div>
                                    {{/unless}}
                                </li>
                            {{/each}}
                        </ol>
                    </div>
                    {{/if}}
                    {{/each}}
                </div>

                {{!-- Combat Tab --}}
                {{#unless printable}}
                <div class="tab combat items" data-group="primary" data-tab="combat">
                {{else}}
                <div class="combat items" style="break-before: page;">
                    <h1>Combat</h1>
                {{/unless}}
                <!-- Combat Maneuvers -->
                <div class="items-list">
                    <header class="items-header flexrow">
                        <h3 class="item-name name">Combat Techniques</h3>
                        <div class="detail ele" data-tooltip="Reach">RCH</div>
                        <div class="detail ele" data-tooltip="Zone Dice">ZD</div>
                        <div class="detail impact" data-tooltip="Impact Formula">Impact</div>
                        <div class="detail ele" data-tooltip="Attack Mastery Level">Atk</div>
                        <div class="detail ele" data-tooltip="Block Mastery Level">Blk</div>
                        <div class="detail ele" data-tooltip="Counterstrike Mastery Level">CX</div>
                        <div class="detail notes">Notes</div>
                        {{#unless printable}}
                        <div class="item-controls">
                            <a class="item-control item-create" data-tooltip="Create Combat Maneuver"
                                data-type="combatmaneuver"><i class="fas fa-file-plus fa-lg"></i></a>
                        </div>
                        {{/unless}}
                    </header>
                    {{#each combatmaneuvers as | cm |}}
                    <ol class="item-list combatmaneuver" data-item-id="{{cm.item.id}}"
                        data-item-uuid="{{cm.item.uuid}}" data-item-name="{{cm.item.name}}">
                        <header class="items-header flexrow">
                            <h3 class="item-name notes">{{cm.item.name}}</h3>
                        </header>
                        {{#each cm.strikeModes as |item|}}
                            <li class="item flexrow" data-item-id="{{item.id}}"
                                data-uuid="{{item.uuid}}" data-item-name="{{item.name}}">
                                <div
                                    class="item-name name default-action flexrow">
                                    <div class="item-image" style="background-image: url('{{item.img}}')"></div>
                                    <h4 data-tooltip="{{item.name}}">{{item.name}}</h4>
                                </div>
                                <div class="detail ele" data-tooltip="{{system.$reach.abbrev}}">{{system.$reach.effective}}</div>
                                <div class="detail ele">
                                    {{#unless (or system.$attack.disabled system.$impact.disabled (eq system.zoneDie 0))}}
                                    <span>d{{system.zoneDie}}</span>
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail impact">
                                    {{#unless (or system.$attack.disabled system.$impact.disabled)}}
                                    <span data-tooltip="{{system.$impact.abbrev}}">{{system.$impact.label}}</span>
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail ele">
                                    {{#unless system.$attack.disabled}}
                                    <span data-tooltip="{{system.$attack.abbrev}}">{{system.$attack.effective}}</span>
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail ele">
                                    {{#unless system.$defense.block.disabled}}
                                    <span data-tooltip="{{system.$defense.block.abbrev}}">{{system.$defense.block.effective}}</span>
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail ele">
                                    {{#unless system.$defense.counterstrike.disabled}}
                                    <span data-tooltip="{{system.$defense.counterstrike.abbrev}}">{{system.$defense.counterstrike.effective}}</span>
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail notes">{{system.notes}}</div>
                                {{#unless printable}}
                                <div class="item-controls">
                                    <a class="item-control item-contextmenu" title="Menu"><i
                                        class="fas fa-ellipsis-vertical fa-fw"></i></a>
                                </div>
                                {{/unless}}
                            </li>
                        {{/each}}
                    </ol>
                    {{/each}}
                </div>
                
                <!-- Melee Weapons -->
                {{#if meleeweapons.length}}
                <div class="items-list">
                    <header class="items-header flexrow">
                        <h3 class="item-name name">Melee Strike Modes</h3>
                        <div class="detail ele" data-tooltip="Heft">HFT</div>
                        <div class="detail ele" data-tooltip="Reach">RCH</div>
                        <div class="detail ele" data-tooltip="Zone Dice">ZD</div>
                        <div class="detail impact" data-tooltip="Impact Formula">Impact</div>
                        <div class="detail ele" data-tooltip="Attack Mastery Level">Atk</div>
                        <div class="detail ele" data-tooltip="Block Mastery Level">Blk</div>
                        <div class="detail ele" data-tooltip="Counterstrike Mastery Level">CX</div>
                        <div class="detail notes">Notes</div>
                    </header>
                    {{#each meleeweapons as | weapon |}}
                    <ol class="item-list weapon" data-item-id="{{weapon.item.id}}"
                        data-item-uuid="{{weapon.item.uuid}}" data-item-name="{{weapon.item.name}}">
                        <header class="items-header flexrow">
                            <h3 class="item-name notes">{{weapon.item.name}}</h3>
                        </header>
                        {{#each weapon.strikeModes as |item|}}
                            <li class="item flexrow"
                                data-item-id="{{item.id}}" data-uuid="{{item.uuid}}" data-item-name="{{item.name}}">
                                <div
                                    class="item-name name default-action flexrow">
                                    <div class="item-image" style="background-image: url('{{item.img}}')"></div>
                                    <h4 data-tooltip="{{item.name}}">{{item.name}}</h4>
                                </div>
                                <div class="detail ele" data-tooltip="{{system.$heft.abbrev}}">{{system.$heft.effective}}</div>
                                <div class="detail ele" data-tooltip="{{system.$reach.abbrev}}">{{system.$reach.effective}}</div>
                                <div class="detail ele">d{{system.zoneDie}}</div>
                                <div class="detail impact" data-tooltip="{{system.$impact.abbrev}}">
                                    {{#unless system.$impact.disabled}}
                                    {{system.$impact.label}}
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail ele" data-tooltip="{{system.$attack.abbrev}}">
                                    {{#unless system.$attack.disabled}}
                                    {{system.$attack.effective}}
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail ele" data-tooltip="{{system.$defense.block.abbrev}}">
                                    {{#unless system.$defense.block.disabled}}
                                    {{system.$defense.block.effective}}
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail ele" data-tooltip="{{system.$defense.counterstrike.abbrev}}">
                                    {{#unless system.$defense.counterstrike.disabled}}
                                    {{system.$defense.counterstrike.effective}}
                                    {{else}}
                                    <i class="fas fa-xmark fa-lg"></i>
                                    {{/unless}}
                                </div>
                                <div class="detail notes">{{notes}}</div>
                                {{#unless printable}}
                                <div class="item-controls">
                                    <a class="item-control item-contextmenu" title="Menu"><i
                                        class="fas fa-ellipsis-vertical fa-fw"></i></a>
                                </div>
                                {{/unless}}
                            </li>
                        {{/each}}
                    </ol>
                    {{/each}}
                </div>
                {{/if}}

                <!-- Missiles -->
                {{#if itemTypes.missilestrikemode.length}}
                <div class="items-list">
                    <header class="items-header flexrow">
                        <h3 class="item-name name">Ranged Strike Modes</h3>
                        <div class="detail ele">Pull</div>
                        <div class="detail ele">Draw</div>
                        <div class="detail ele">BR</div>
                        <div class="detail ele">MaxVM</div>
                        <div class="detail impact">Impact</div>
                        <div class="detail ele">Atk</div>
                        <div class="detail notes">Notes</div>
                    </header>
                    {{#each missileweapons as | missile |}}
                    <ol class="item-list missile" data-item-id="{{missile.item.id}}"
                        data-item-uuid="{{missile.item.uuid}}" data-item-name="{{missile.item.name}}">
                        <header class="items-header flexrow">
                            <h3 class="item-name notes">{{missile.item.name}}</h3>
                        </header>
                        {{#each missile.strikeModes as |item|}}
                            <li class="item flexrow"
                                data-item-id="{{item.id}}" data-uuid="{{item.uuid}}" data-item-name="{{item.name}}" data-weapon-id="{{missile.weaponId}}"
